{
  "name": "ChronoFeed",
  "version": "1.0.0",
  "description": "Fitness and Health",
  "main": "app.js",
  "scripts": {
    "app-watch": "nodemon -w app.js -w datastore app.js",
    "live-reload": "live-reload app.js datastore public --port=1009",
    "build-watch": "npm run build && watch-spawn -p webpack.config.js -p controllers/**/* -p model/*.js -p scripts/*.js npm run build",
    "test-watch": "npm run test && watch-spawn -p .jshintrc -p *.js -p controllers/**/*.js -p model/**/*.js -p datastore/**/*.js -p scripts/**/*.js npm run test",
    "build": "npm run webpack && npm run uglify",
    "test": "npm run lint && npm run jasmine",
    "lint": "npm run jshint && npm run eslint",
    "jshint": "npm run jshint-node && npm run jshint-browser",
    "unit:test": "npm run lint && npm run test:build && npm run test:copy && npm run jasmine",
    "test:copy": "node_modules/.bin/n-copy --source public/bower_components/moment --destination build/public/bower_components/moment '*.js'",
    "test:build": "node_modules/.bin/babel --presets es2015 model -d build/model",
    "webpack": "node_modules/.bin/webpack",
    "uglify": "node_modules/.bin/uglifyjs public/scripts/chronofeed.js -o public/scripts/chronofeed.min.js",
    "jasmine": "node_modules/.bin/jasmine",
    "jshint-node": "node_modules/.bin/jshint --config .jshintrc-node app.js model datastore",
    "jshint-browser": "node_modules/.bin/jshint --config .jshintrc-browser controllers scripts",
    "eslint": "node_modules/.bin/eslint app.js controllers model scripts datastore",
    "wc": "git ls-files | grep .js | xargs wc -l",
    "copy": "npm run copy-bootstrap-css && npm run copy-bootstrap-fonts && npm run copy-html && npm run copy-json-editor && npm run copy-css && npm run copy-fonts && npm run copy-datetimepicker-css",
    "copy-bootstrap-css": "node_modules/.bin/n-copy -s bower_components/bootstrap/dist/css -d public/bootstrap/css \"*.css\"",
    "copy-bootstrap-fonts": "node_modules/.bin/n-copy -s bower_components/bootstrap/dist/fonts -d public/bootstrap/fonts \"*\"",
    "copy-html": "n-copy -s html -d public \"*.html\"",
    "copy-json-editor": "n-copy -s bower_components/json-editor/dist -d public/json-editor \"*.js\"",
    "copy-css": "n-copy -s css -d public/css \"*.css\"",
    "copy-fonts": "n-copy -s css/fonts -d public/css/fonts \"*\"",
    "copy-datetimepicker-css": "n-copy -s bower_components/eonasdan-bootstrap-datetimepicker/build/css -d public/datetimepicker/css \"*.css\"",
    "fp": "npm run fp-bootstrap-css && npm run fp-datetimepicker-css && npm run fp-css && npm run fp-fonts && npm run fp-chronofeed-js && npm run fp-json-editor",
    "fp-bootstrap-css": "node assetfp.js -w public bootstrap/css/bootstrap.min.css -o manifest.json -a -n bootstrap",
    "fp-datetimepicker-css": "node assetfp -w public datetimepicker/css/bootstrap-datetimepicker.min.css -o manifest.json -a -b eonasdan-bootstrap-datetimepicker",
    "fp-css": "node assetfp -w public css/icomoon.css -o manifest.json -a",
    "fp-fonts": "node assetfp -w public css/fonts -o manifest.json -a",
    "fp-chronofeed-js": "node assetfp -w public scripts/chronofeed.min.js -o manifest.json -a",
    "fp-json-editor": "node assetfp -w public json-editor/jsoneditor.min.js -o manifest.json -a -b json-editor",
    "sub-html": "node assetfp-sub.js public/manifest.json public/index.html"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dabowheel/chronofeed.git"
  },
  "author": "Miguel Provencio",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dabowheel/chronofeed/issues"
  },
  "homepage": "https://github.com/dabowheel/chronofeed",
  "dependencies": {
    "babel-polyfill": "^6.0.14",
    "bootstrap": "^3.3.5",
    "bootstrap-webpack": "0.0.5",
    "compression": "^1.6.0",
    "connect-mongo": "^0.8.2",
    "cookie-parser": "^1.3.5",
    "exports-loader": "^0.6.2",
    "express": "^4.12.4",
    "express-session": "^1.11.2",
    "extract-text-webpack-plugin": "^0.9.1",
    "file-loader": "^0.8.4",
    "handlebars-loader": "^1.1.4",
    "imports-loader": "^0.6.5",
    "jquery": "^2.1.4",
    "less": "^2.5.3",
    "less-loader": "^2.2.1",
    "moment": "^2.10.6",
    "mongodb": "^2.0.35",
    "nodemailer": "^1.8.0",
    "nodemailer-smtp-transport": "^1.0.3",
    "url-loader": "^0.5.6",
    "vue": "^1.0.10"
  },
  "devDependencies": {
    "babel-core": "^6.2.1",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.1.18",
    "css-loader": "^0.22.0",
    "eslint": "^1.7.3",
    "html-loader": "^0.3.0",
    "jasmine": "^2.3.2",
    "jshint": "^2.8.0",
    "minimist": "^1.2.0",
    "npm-build-tools": "^2.2.5",
    "style-loader": "^0.13.0",
    "uglify-js": "^2.5.0",
    "webpack": "^1.12.2"
  }
}
