{
  "name": "ChronoFeed",
  "version": "1.0.0",
  "description": "Fitness and Health",
  "main": "app.js",
  "scripts": {
    "app-watch": "node_modules/.bin/nodemon -w app.js -w api -w mongodb app.js",
    "test": "npm run jasmine && npm run mocha",
    "lint": "npm run eslint && npm run jshint",
    "jshint": "npm run jshint-node && npm run jshint-browser && npm run jshint-vue && npm run jshint-html",
    "unit:test": "npm run lint && npm run test:build && npm run test:copy && npm run jasmine",
    "test:copy": "node_modules/.bin/n-copy --source public/bower_components/moment --destination build/public/bower_components/moment '*.js'",
    "test:build": "node_modules/.bin/babel --presets es2015 model -d build/model",
    "jasmine": "node_modules/.bin/jasmine",
    "mocha": "node_modules/.bin/mocha mongodb/spec api/spec",
    "jshint-node": "node_modules/.bin/jshint --config .jshintrc-node app.js assetfp.js assetfp-sub.js api mongodb",
    "jshint-vue": "node_modules/.bin/jshint -c .jshintrc-browser --extract always components -e vue",
    "jshint-html": "node_modules/.bin/jshint -c .jshintrc-browser --extract always assets -e html",
    "jshint-browser": "node_modules/.bin/jshint --config .jshintrc-browser components",
    "eslint": "node_modules/.bin/eslint *.js api components assets/scripts",
    "wc": "git ls-files | grep .js | xargs wc -l",
    "copy": "npm run copy-assets && npm run copy-json-editor",
    "copy-assets": "node_modules/.bin/cpx \"assets/**/*\" public",
    "copy-json-editor": "node_modules/.bin/cpx bower_components/json-editor/dist/jsoneditor.min.js public/vendor",
    "fp-images": "node assetfp -w public images -o manifest.json -a",
    "fp-js": "node assetfp -w public scripts -o manifest.json -a",
    "fp-assets": "npm run fp-images && npm run fp-js",
    "fp-json-editor": "node assetfp -w public vendor/jsoneditor.min.js -o manifest.json -a -b json-editor",
    "sub-html": "node assetfp-sub.js public/manifest.json public/index.html public/login.html public/signup.html public/loglist.html public/log.html public/designer.html",
    "clean-assets": "node_modules/.bin/n-clean public",
    "build-assets": "npm run copy && npm run fp-assets && npm run fp-json-editor && npm run sub-html",
    "build": "npm run clean-assets && npm run webpack && npm run copy-webpack && npm run build-assets",
    "webpack": "node_modules/.bin/webpack",
    "copy-webpack": "node_modules/.bin/cpx \"build/scripts/*\" public/scripts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dabowheel/chronofeed.git"
  },
  "author": "Miguel Provencio",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dabowheel/chronofeed/issues"
  },
  "homepage": "https://github.com/dabowheel/chronofeed",
  "dependencies": {
    "babel-polyfill": "^6.0.14",
    "body-parser": "^1.14.1",
    "compression": "^1.6.0",
    "connect-mongo": "^0.8.2",
    "cookie-parser": "^1.3.5",
    "exports-loader": "^0.6.2",
    "express": "^4.12.4",
    "express-session": "^1.11.2",
    "extract-text-webpack-plugin": "^0.9.1",
    "file-loader": "^0.8.4",
    "handlebars-loader": "^1.1.4",
    "imports-loader": "^0.6.5",
    "jquery": "^2.1.4",
    "less": "^2.5.3",
    "less-loader": "^2.2.1",
    "moment": "^2.10.6",
    "mongodb": "^2.0.35",
    "nodemailer": "^1.8.0",
    "nodemailer-smtp-transport": "^1.0.3",
    "url-loader": "^0.5.6",
    "vue": "^1.0.10"
  },
  "devDependencies": {
    "babel-core": "^6.2.1",
    "babel-loader": "^6.2.0",
    "babel-plugin-transform-runtime": "^6.1.18",
    "babel-preset-es2015": "^6.1.18",
    "babel-runtime": "^6.2.0",
    "cpx": "^1.2.1",
    "css-loader": "^0.23.0",
    "es-promise-http": "^0.2.0",
    "eslint": "^1.7.3",
    "html-loader": "^0.3.0",
    "jasmine": "^2.3.2",
    "jshint": "^2.8.0",
    "minimist": "^1.2.0",
    "mocha": "^2.3.4",
    "nodemon": "^1.8.1",
    "npm-build-tools": "^2.2.5",
    "sha.js": "^2.4.4",
    "style-loader": "^0.13.0",
    "template-html-loader": "0.0.3",
    "uglify-js": "^2.5.0",
    "vue-async-data": "^1.0.2",
    "vue-hot-reload-api": "^1.2.2",
    "vue-html-loader": "^1.0.0",
    "vue-loader": "^7.1.7",
    "webpack": "^1.12.2"
  }
}
