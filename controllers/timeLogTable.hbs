<div class="container">
	<div class="col-lg-6 col-md-7 col-sm-8 col-xs-9">
		<table class="table table-hover table-bordered">
			<thead>
				<tr>
					<th>Time</th>
					<th>Duration</th>
					<th>Event</th>
				</tr>
			</thead>
			<tbody>
				{{#each events}}
				<tr>
					<td>{{time}}</td>
					<td>{{duration}}</td>
					<td>{{name}}</td>
				</tr>
				{{/each}}
			</tbody>
		</table>
	</div>
</div>
